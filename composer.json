{
  "name": "lampatrampa/php-grpc",
  "type": "server",
  "description": "Forked from spiral/php-grpc GRPC server for PHP applications",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Titov / Wolfy-J",
      "email": "wolfy.jd@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "google/protobuf": "^3.7",
    "spiral/roadrunner": "^1.8"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "spiral/code-style": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\GRPC\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": "tests/src/",
      "Spiral\\GRPC\\Tests\\": "tests/GRPC/"
    }
  }
}
